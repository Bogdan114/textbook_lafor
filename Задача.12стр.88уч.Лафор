#include <iostream>

using namespace std;

int main()
{
	system("chcp 1251>nul");
	float a;
	int star_f, star_sh, star_p;
	a = 3.51;
	star_sh = (a - static_cast<int>(a))*100;
	star_p = star_sh % 5*2;
	star_sh = star_sh / 5;
	star_f = a / 1;
	cout << "J" << star_f << "." << star_sh << "." << star_p;
	system("pause>nul");
	return 0;
}
